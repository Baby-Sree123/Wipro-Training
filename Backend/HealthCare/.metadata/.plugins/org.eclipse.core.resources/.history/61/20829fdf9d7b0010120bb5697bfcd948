package com.example.doctor_service.model;

import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;

@Entity
public class Doctor {
	
	 @Id
	    @GeneratedValue(strategy = GenerationType.IDENTITY)
	    private Long id;

	    private String doctname;
	    private String doctemail;
	    private String doctphone;

	   
	    private String specialization;

	    
	    private String schedule;

		public Long getId() {
			return id;
		}

		public void setId(Long id) {
			this.id = id;
		}

		public String getDoctname() {
			return doctname;
		}

		public void setDocname(String docname) {
			this.doctname = docname;
		}

		public String getDocemail() {
			return doctemail;
		}

		public void setDocemail(String docemail) {
			this.doctemail = docemail;
		}

		public String getDocphone() {
			return doctphone;
		}

		public void setDocphone(String docphone) {
			this.doctphone = docphone;
		}

		public String getSpecialization() {
			return specialization;
		}

		public void setSpecialization(String specialization) {
			this.specialization = specialization;
		}

		public String getSchedule() {
			return schedule;
		}

		public void setSchedule(String schedule) {
			this.schedule = schedule;
		}

		public Doctor(Long id, String docname, String docemail, String docphone, String specialization,
				String schedule) {
			super();
			this.id = id;
			this.doctname = docname;
			this.doctemail = docemail;
			this.doctphone = docphone;
			this.specialization = specialization;
			this.schedule = schedule;
		}
	    
		public Doctor() {}

}